when HTTP_PROXY_REQUEST {
    switch [class match [string tolower [HTTP::uri]] contains Shared/explicitProxy_allowed_URLs_datagroup] {
        1 {
            log local0. "ALLOWED - URL: [HTTP::uri]"
            }
        default {
            log local0. "DENIED - URL: [HTTP::uri] [client_addr]"
            HTTP::respond 403
            }
    }
}
